<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="600.0" prefWidth="500.0" styleClass="background" stylesheets="@application.css" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="controller.MainWindowController">
   <children>
      <TableView fx:id="tableView" layoutX="50.0" layoutY="210.0" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="50.0">
        <columns>
          <TableColumn fx:id="firstNameColumn" prefWidth="200.0" text="First Name" />
          <TableColumn fx:id="lastNameColumn" prefWidth="200.0" text="Last Name" />
            <TableColumn fx:id="ageColumn" prefWidth="100.0" text="Age" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="closeButton" layoutX="14.0" layoutY="6.0" mnemonicParsing="false" onAction="#closeStage" style="-fx-font-size: 20; -fx-font-weight: bold;" text="Close" />
   </children>
</AnchorPane>